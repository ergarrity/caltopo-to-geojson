    "test": "npm run compile-lib && babel-node test/index.js"
  },
  "dependencies": {
    "xml2js": "0.4.16"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "tape": "4.5.1"
  },
  "license": "MIT"
}
